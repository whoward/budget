- model_class = ImportService

.page-header
  %h1= t '.title', :default => model_class.model_name.human.pluralize.titleize

%table.table.table-striped
  %thead
    %tr
      %th= model_class.human_attribute_name(:active)
      %th= model_class.human_attribute_name(:name)
      %th= model_class.human_attribute_name(:type)
      %th= t '.actions', :default => t("helpers.actions")

  %tbody
    - @import_services.each do |service|
      %tr
        %td= service.active
        %td= service.name
        %td= service.type
        %td
          = link_to_edit_resource(service)
          = link_to_delete_resource(service)
        
= link_to t('.new', default: t("helpers.links.new")), new_import_service_path, class: 'btn btn-primary'
